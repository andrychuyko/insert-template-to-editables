{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "1",
    "homepage_url": "https://github.com/kyuucr/insert-template-to-editables",
    "default_locale": "en",
    "applications": {
        "gecko": {
            "id": "{7aca7ac1-ccce-46a2-a14e-f477daa08d32}",
            "strict_min_version": "48.0"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "browser_style": true,
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/insert-dark.svg",
            "32": "icons/insert-dark.svg"
        }
    },
    "options_ui": {
          "page": "options.html",
          "browser_style": true
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "notifications",
        "storage"
    ]
}
